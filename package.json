{
  "name": "jira-branchname-bookmarklet",
  "version": "1.0.0",
  "description": "Bookmarklet to quickly copy Git branch names from Jira issues.",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "minify": "npx terser src/branchname.js -c -m -o dist/branchname.min.js",
    "bookmarklet": "npx bookmarklet dist/branchname.min.js > dist/branchname.bookmarklet.txt",
    "build": "npm run clean && npm run minify && npm run bookmarklet"
  },
  "devDependencies": {
    "bookmarklet": "^3.0.0",
    "terser": "^5.31.0"
  },
  "keywords": ["jira", "bookmarklet", "branch", "git"],
  "license": "MIT"
}
